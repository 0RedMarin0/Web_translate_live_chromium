{
    "name": "Image Grabber",
    "description": "Extract all images from current web page",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icon/icon-2.png",
        "32":"icon/icon-2.png",
        "48":"icon/icon-2.png",
        "128":"icon/icon-2.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [],
    "background": {
      "service_worker": "service_worker.js",
      "type": "module"
    }

}
